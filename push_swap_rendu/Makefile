ERASE        = \033[2K\r

NAME = push_swap

FLAGS = -Wall -Wextra -Werror -I .

SRCS =	main.c \
		src/push_swap.c \
		src/debug.c \
		src/parsing/parsing.c \
		src/parsing/verif_val.c \
		src/parsing/ft_split_spe.c \
		src/libft/ft_atol.c \
		src/libft/ft_bzero.c \
		src/libft/ft_calloc.c \
		src/libft/ft_isnumber.c \
		src/libft/ft_lstadd_back.c \
		src/libft/ft_lstadd_front.c \
		src/libft/ft_lstclear.c \
		src/libft/ft_lstdelone.c \
		src/libft/ft_lstlast.c \
		src/libft/ft_lstbeforelast.c \
		src/libft/ft_lstnew.c \
		src/libft/ft_lstsize.c \
		src/libft/ft_putstr.c \
		src/libft/ft_strcmp.c \
		src/libft/ft_strjoin.c \
		src/libft/ft_strlen.c \
		src/libft/ft_substr.c \
		src/libft/ft_lstinit.c \
		src/push_swap_utils/push.c \
		src/push_swap_utils/reverse.c \
		src/push_swap_utils/rotate.c \
		src/push_swap_utils/swap.c \
		src/push_swap_utils/push_swap_utils.c \
		src/push_swap_utils/smart_sort_tools.c \
		src/push_swap_utils/five_hundred_sort.c \

OBJS = ${SRCS:.c=.o}

HEADER = src/push_swap.h

all :	${NAME}

# $@ : NAME de la regle
# $^ : tous les OBJS
${NAME}:	${OBJS}
		@ ${CC} ${FLAGS} $^ -o $@ && echo \\npush_swap compilation...\\n\\n*===============*

# cree les objets
# $< : premiere dependance
# ${HEADER} Makefile : pour eviter les relinks
%.o:	%.c ${HEADER} Makefile
		@ ${CC} ${FLAGS} -c $< -o $@ && echo $@

clean:	
		@ ${RM} ${OBJS} && echo clean...

fclean:	clean
		@ ${RM} ${NAME} && echo full clean...

re:	fclean
	${MAKE} all

hundred: hundred1 hundred2 hundred3 hundred4

